{
  "name": "Ihc",
  "version": "0.0.1",
  "private": true,
  "babel": {
    "presets": [
      "react-native"
    ]
  },
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "testService": "jest LocalDataService",
    "updateSnapshots": "jest -u",
    "lint": "eslint components/* models/* screens/* services/* util/* redux*/*",
    "lintFix": "eslint --fix components/* models/* screens/* services/* /util/*",
    "check": "npm run test && npm run lint"
  },
  "dependencies": {
    "jest-cli": "^21.2.1",
    "npm": "^6.0.0",
    "prop-types": "^15.6.0",
    "react": "16.0.0",
    "react-devtools": "^3.2.1",
    "react-native": "^0.53.3",
    "react-native-easy-grid": "^0.1.17",
    "react-native-modal": "^10.0.0",
    "react-native-navigation": "^1.1.302",
    "react-native-wheel-picker": "^1.2.0",
    "react-redux": "^5.0.7",
    "realm": "^2.3.3",
    "redux": "^4.0.0",
    "tcomb-form-native": "^0.6.11"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "babel-jest": "^21.2.0",
    "babel-preset-react-native": "4.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-react-native": "^3.2.1",
    "jest": "^21.2.1",
    "jest-fetch-mock": "^1.5.0",
    "jest-serializer-enzyme": "^1.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.3.1",
    "react-native-mock": "^0.3.1",
    "react-test-renderer": "16.0.0",
    "sinon": "^4.5.0"
  },
  "jest": {
    "preset": "./react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|tcomb-form-native)"
    ],
    "globals": {
      "__TEST__": true
    },
    "setupTestFrameworkScriptFile": "<rootDir>/jest-setup.js"
  }
}
